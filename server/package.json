{
  "name": "servertemplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Odee <odjhey@github.com>",
  "license": "MIT",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "watch": "nodemon src/server.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src -d dist --extensions '.ts,.tsx' --ignore '**/*.spec.ts,**/*.test.ts' --source-maps inline",
    "test": "jest",
    "test:w": "jest --watch",
    "coverage": "jest --coverage && jest-coverage-badges input coverage/coverage-summary.json output badges && open coverage/index.html",
    "cov:silent": "jest --coverage && jest-coverage-badges input coverage/coverage-summary.json output badges"
  },
  "config": {
    "nodemonConfig": {
      "verbose": true,
      "ignore": [
        "**/*.test.ts",
        "**/*.spec.ts",
        ".git",
        "node_modules"
      ],
      "watch": [
        "src"
      ],
      "exec": "npm start",
      "ext": "ts"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.7.4",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.25",
    "@types/mongodb": "^3.3.6",
    "@types/mongoose": "^5.5.20",
    "@types/node": "^12.7.12",
    "apollo-server-testing": "^2.9.16",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "graphql-tag": "^2.10.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-coverage-badges": "^1.1.2",
    "nock": "^11.7.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "add": "^2.0.6",
    "apollo-server": "^2.9.6",
    "atob": "^2.1.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "googleapis": "^48.0.0",
    "graphql": "^14.5.8",
    "gsheeez": "github:odjhey/gsheeez",
    "merge-graphql-schemas": "^1.7.6",
    "moment": "2.24.0",
    "mongoose": "^5.7.4",
    "node-blob": "0.0.2",
    "serve-index": "^1.9.1",
    "yarn": "^1.22.4"
  }
}
